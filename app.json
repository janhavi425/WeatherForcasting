{
  "expo": {
    "name": "Weather",
    "slug": "Weather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/weather-app.png",
    "githubUrl":"https://github.com/janhavi425/WeatherForcasting",
    "splash": {
      "image": "./assets/weather-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#37003c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.Weather.Weather",
      "adaptiveIcon": {
        "foregroundImage": "./assets/weather-app.png",
        "backgroundColor": "#37003c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
